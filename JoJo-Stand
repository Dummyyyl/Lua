OWNER.Chatted:Connect(function(msg)
    if msg:lower() == "ping!" then
        local value = game:GetService("Stats").Network.ServerStatsItem["Data Ping"]:GetValue()
        local roundedValue = string.format("%.1f", value)
        game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(roundedValue, 'All')
    end
end)

OWNER.Chatted:Connect(function(msg)
    if msg:lower() == "fps!" then
        local stats = game:GetService("Stats")
        local framerateManager = stats.FrameRateManager
        local fpsvalue = math.floor(1000 / framerateManager.RenderAverage:GetValue() + 0.5)
        game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(tostring(fpsvalue), 'All')        
    end
end)

