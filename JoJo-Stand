game:GetService('RunService').Stepped:connect(function()
    if oranear == true then
        follow = false
        noclip = false
        autostomp = true

        local target = GetNearest2()
        if target then
            local targetchar = target.Character
            if targetchar.BodyEffects['K.O'].Value == true then
                lp.Character.HumanoidRootPart.CFrame = CFrame.new(Target.Character.UpperTorso.Position) + Vector3.new(0,2,0)
            end
        elseif targetchar.BodyEffects['K.O'].Value == true then
            Summon()
            noclip = true
            autostomp = false
        end
    end
end)
end)()

