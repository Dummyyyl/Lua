    game:GetService('RunService').Stepped:connect(function()
        if barragepos == true then
            lp.Character.HumanoidRootPart.CFrame = OWNER.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, -8)
        end
    end)
end)()

