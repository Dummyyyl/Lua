OWNER.Chatted:Connect(function(msg1)
    local msg = string.split(msg1," ")
    if msg[1]:lower() == custombag and msg[2] then
    killnear        = false
    DeSummon()
    noclip          = false
    bring           = true
    barragepos      = false
    barragepunch    = false
    vanish          = false
    aura            = false
    autostomp       = false
    block           = false
    oaura           = false
    Killing         = false
    pcall(function()
        local Target = GetPlayerByDisplayname(msg[2])
        LPH_NO_VIRTUALIZE(function()
            if Target then
            if game.Players.LocalPlayer.Name == bringalt then
                Play(standbags,true)
            repeat
                game:GetService("RunService").RenderStepped:Wait()
                lp.Character.HumanoidRootPart.CFrame = CFrame.new(game:GetService("Workspace").Ignored.Shop["[BrownBag] - $26"].Head.Position) --Automask
                fireclickdetector(game:GetService("Workspace").Ignored.Shop["[BrownBag] - $26"].ClickDetector)
            until lp.Backpack:FindFirstChild("[BrownBag]")
            local humanoidRootPart = lp.Character.HumanoidRootPart
            local targetHrp = Target.Character.HumanoidRootPart
            repeat
                local TargetVel = Target.Character.HumanoidRootPart.Velocity
                local TargetPos = Target.Character.HumanoidRootPart.Position+Vector3.new(TargetVel.X*PredictionValue, 0, TargetVel.Z*PredictionValue)
                humanoidRootPart.CFrame = CFrame.new(TargetPos)
                if lp.Backpack:FindFirstChild('[BrownBag]') then
                    lp.Backpack:FindFirstChild('[BrownBag]').Parent = lp.Character
                    lp.Character:FindFirstChild('[BrownBag]'):Activate()
                else
                    lp.Character:FindFirstChild('[BrownBag]'):Activate()
                end
                task.wait(0.01)
            until Target.Character:FindFirstChild("Christmas_Sock") or not lp.Character:FindFirstChild("Christmas_Sock") and  lp.Backpack:FindFirstChild("[BrownBag]")
        else
            bring = false
            Summon()
        end
        bring = false
            Summon()
                else
                    if Chatting then
                        game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Invalid Target.", 'All')
                    end
                    bring = false
                    Summon()
                end
                end)()
            end)
        end
    end)

--//-------------------------------------------------------------------------------------------------------------\\--

OWNER.Chatted:Connect(function(msg1)
    local msg = string.split(msg1," ")
        if msg[1]:lower() == customcheck and msg[2] then
            pcall(function()
                local Target = GetPlayerByDisplayname(msg[2])
                LPH_NO_VIRTUALIZE(function()
                if Target then
                    game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Target is in the server.", 'All')
                    Play(standchecks,true)
                else
                    game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Target is not in the server.", 'All')
                    Play(standchecks,true)
                end
            end)()
        end)
    end
end)

