            end
        else
        if Chatting then
            game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Anything for my Master!", 'All')
        end
        Play(standdropcashs,true)
            DropMoney(10000)
        end
    end
end)

