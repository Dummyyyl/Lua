OWNER.Chatted:Connect(function(msg1)
    local msg = string.split(msg1," ")
    if msg[1]:lower() == customarrest and msg[2] then
        killnear        = false
        DeSummon()
    
        noclip          = false
        bring           = true
        barragepos      = false
        barragepunch    = false
        vanish          = false
        aura            = false
        autostomp       = false
        block           = false
        oaura           = false
        Killing         = false
        pcall(function()
            local Target = GetPlayerByDisplayname(msg[2])
            LPH_NO_VIRTUALIZE(function()
                if Target then
                    if lp.DataFolder.Officer.Value == 1 then
                        if Target.DataFolder.Officer.Value == 1 then
                            if Chatting then
                                game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Target is a police officer.", 'All')
                            end
                            Summon()
                            block = true
                        else
                            if Target.leaderstats.Wanted.Value == 0 then
                                if Chatting then
                                    game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Target has 0 Bounty.", 'All')
                                end
                                Summon()
                                block = true
                            else
                                Play(standarrests, false)
                                local humanoidRootPart = lp.Character.HumanoidRootPart
                                local targetHrp = Target.Character.HumanoidRootPart
                                while Target.Character.BodyEffects["K.O"].Value == false and bring == true do
                                    DeSummon()
        
                                    if lp.Backpack:FindFirstChild('Combat') then
                                        lp.Backpack:FindFirstChild('Combat').Parent = lp.Character
                                        lp.Character:FindFirstChild('Combat'):Activate()
                                    else
                                        lp.Character:FindFirstChild('Combat'):Activate()
                                    end
        
                                    for i = 1, 9 do
                                        lp.Character.HumanoidRootPart.CFrame = CFrame.new(Target.Character.UpperTorso.Position) * CFrame.new(0, 5000, 0)
                                        if bring == false then Summon() return end
                                        wait(.1)
                                    end
                                    wait(.01)
                                    for i = 1, 30 do
                                        local TargetVel = Target.Character.HumanoidRootPart.Velocity
                                        local TargetPos = Target.Character.HumanoidRootPart.Position+Vector3.new(TargetVel.X*PredictionValue, 0, TargetVel.Z*PredictionValue)
                                        humanoidRootPart.CFrame = CFrame.new(TargetPos)
                                        if bring == false then Summon() return end
                                        wait(.01)
                                    end
                                    wait(.1)
                                    for i = 1, 14 do
                                        lp.Character.HumanoidRootPart.CFrame = CFrame.new(Target.Character.UpperTorso.Position) * CFrame.new(0, 5000, 0)
                                        if bring == false then Summon() return end
                                        wait(.1)
                                    end
                                end
        
                                if bring == false then return end
        
                                lp.Character:WaitForChild("Humanoid"):ChangeState(11)
        
                                if game.Players.LocalPlayer.Name == bringalt then
                                    while Target.Character.BodyEffects.Dead.Value == false and bring == true and not Target.Character:FindFirstChild("GRABBING_CONSTRAINT") and Target.Character.BodyEffects.Cuff.Value == false do
                                        lp.Character.HumanoidRootPart.CFrame = CFrame.new(Target.Character.UpperTorso.Position) * CFrame.new(0,2.5,0)
                                        if bring == false then Summon() return end
                                        if lp.Backpack:FindFirstChild('Cuff') then
                                            lp.Backpack:FindFirstChild('Cuff').Parent = lp.Character
                                            lp.Character:FindFirstChild('Cuff'):Activate()
                                        else
                                            lp.Character:FindFirstChild('Cuff'):Activate()
                                        end
        
                                        wait(.1)
                                    end
                                else
                                    Summon()
                                end
        
                                lp.Character:WaitForChild("Humanoid"):ChangeState(12)
                                Summon()
                                block = true
                            end
                        end
                    else
                        if Chatting then
                            game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("I'm not a police officer, Master!", 'All')
                        end
                        Summon()
                        block = true
                    end
                else
                    if Chatting then
                        game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Invalid Target.", 'All')
                    end
                    Summon()
                    block = true
                    Killing = false
                end
            end)
        end)        
    end
end)

