OWNER.Chatted:Connect(function(msg1)
local msg = string.split(msg1," ")
if msg[1]:lower() == customknock and msg[2] then
killnear        = false
DeSummon()
noclip          = false
bring           = true
barragepos      = false
barragepunch    = false
vanish          = false
aura            = false
autostomp       = false
block           = false
oaura           = false
Killing         = false
annoy           = false

pcall(function()
    local Target = GetPlayerByDisplayname(msg[2])
    LPH_NO_VIRTUALIZE(function()
        if Target then
            local humanoidRootPart = lp.Character.HumanoidRootPart
            local targetHrp = Target.Character.HumanoidRootPart
            local TargetVel = Target.Character.HumanoidRootPart.Velocity
            Play(standknocks, true)
            local bringLoop = true
            while bringLoop do
                if lp.Backpack:FindFirstChild('Combat') then
                    lp.Backpack:FindFirstChild('Combat').Parent = lp.Character
                    lp.Character:FindFirstChild('Combat'):Activate()
                else
                    lp.Character:FindFirstChild('Combat'):Activate()
                end
                for i = 1, 9 do
                    lp.Character.HumanoidRootPart.CFrame = CFrame.new(Target.Character.UpperTorso.Position) * CFrame.new(0, 5000, 0)
                    if Target.Character.BodyEffects["K.O"].Value == true or bring == false then Summon() return end
                    wait(.1)
                end
                wait(.01)
                for i = 1, 30 do
                    local TargetVel = Target.Character.HumanoidRootPart.Velocity
                    local TargetPos = Target.Character.HumanoidRootPart.Position+Vector3.new(TargetVel.X*PredictionValue, 0, TargetVel.Z*PredictionValue)
                    humanoidRootPart.CFrame = CFrame.new(TargetPos)
                    wait(0.01)
                    if Target.Character.BodyEffects["K.O"].Value == true or bring == false then Summon() return end
                end
                wait(.1)
                for i = 1, 14 do
                    lp.Character.HumanoidRootPart.CFrame = CFrame.new(Target.Character.UpperTorso.Position) * CFrame.new(0, 5000, 0)
                    wait(.1)
                    if Target.Character.BodyEffects["K.O"].Value == true or bring == false then Summon() return end
                end
                bringLoop = bring and (Target.Character.BodyEffects["K.O"].Value ~= true)
            end
            Summon()
        else
            if Chatting then
                game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Invalid Target.", 'All')
            end
                bring = false
                Summon()
                end
            end)()
        end)
    end
end)

OWNER.Chatted:Connect(function(msg)
    if msg:lower() == customunjail then
        if lp.DataFolder.Currency.Value <= 129 then
            if Chatting then
                game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Not enough money.", 'All')
            end
                else
                if lp.Character.BodyEffects.Cuff.Value == false then
                    if Chatting then
                        game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("I'm not handcuffed, master!", 'All')
                    end
                else
                killnear        = false
                DeSummon()
                noclip          = false
                bring           = false
                barragepos      = false
                barragepunch    = false
                vanish          = false
                aura            = false
                autostomp       = false
                block           = false
                oaura           = false
                Killing         = false
                annoy           = false
                Play(standunjails,true)
                LPH_NO_VIRTUALIZE(function()
                    while not lp.Backpack:FindFirstChild("[Key]") do
                        game:GetService("RunService").RenderStepped:Wait()
                        lp.Character.HumanoidRootPart.CFrame = CFrame.new(game:GetService("Workspace").Ignored.Shop["[Key] - $129"].Head.Position) --Automask
                        fireclickdetector(game:GetService("Workspace").Ignored.Shop["[Key] - $129"].ClickDetector)
                    end
                end)()
                Summon()
                wait(.2)
                lp.Backpack:FindFirstChild('[Key]').Parent = lp.Character
            end
        end
    end
end)

