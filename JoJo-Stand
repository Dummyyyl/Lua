--//-------------------------------------------------------------------------------------------------------------\\--

OWNER.Chatted:Connect(function(msg)
    if msg:lower() == "circle!" then
    DeSummon()
    killnear        = false
    bring           = false
    barragepos      = false
    barragepunch    = false
    vanish          = false
    aura            = false
    autostomp       = false
    block           = false
    oaura           = false
    Summoned2       = true
        game:GetService('RunService').Stepped:connect(function()
            if Summoned2 == true  then
                if lp.Backpack:FindFirstChild('Combat') then
                    lp.Backpack:FindFirstChild('Combat').Parent = lp.Character
                    lp.Character:FindFirstChild('Combat'):Activate()
                else
                    lp.Character:FindFirstChild('Combat'):Activate()
                end
                local orbitRadius = 5
                local orbitSpeed = 6
                        local angle = tick() * orbitSpeed
                        local offsetX = math.cos(angle) * orbitRadius
                        local offsetZ = math.sin(angle) * orbitRadius
                        local ownerPosition = OWNER.Character and OWNER.Character.HumanoidRootPart.Position
                        if ownerPosition then
                        local newPosition = Vector3.new(ownerPosition.X + offsetX, ownerPosition.Y , ownerPosition.Z + offsetZ)
                        lp.Character.HumanoidRootPart.CFrame = CFrame.new(newPosition)
                        local lookVector = (ownerPosition - newPosition).unit
                        local upVector = Vector3.new(0, 1, 0)
                        local newCFrame = CFrame.lookAt(newPosition, ownerPosition, upVector)
                        lp.Character.HumanoidRootPart.CFrame = newCFrame
                    end
                wait()
            end
        end)
    end
end)
    
--//-----------------------------------------------------------------\\--unattack
    
    OWNER.Chatted:Connect(function(msg)
    if msg:lower() == "uncircle!" then
    killnear        = false
    bring           = false
