--//------------------------------------------------------------------------------------------\\--Cash Aura

OWNER.Chatted:Connect(function(msg)
    if msg:lower() == customcashaura then
        Play(standcashauras,true)
        if Chatting then 
            game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Cashaura enabled!", 'All')
        end
            LPH_NO_VIRTUALIZE(function()
            RS:BindToRenderStep("cashaura", -1 , function()
                local function getMoneyAroundMe() 
                    for i, money in ipairs(game.Workspace.Ignored.Drop:GetChildren()) do
                        if money.Name == "MoneyDrop" and (money.Position - lp.Character.HumanoidRootPart.Position).magnitude <= 20 then
                            fireclickdetector(money.ClickDetector)
                        end  
                    end
                end
                getMoneyAroundMe()
            end)
        end)()
    end
end)


