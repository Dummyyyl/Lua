--//------------------------------------------------------------------------------------------\\--Save

OWNER.Chatted:Connect(function(msg)
if msg:lower() == customsave then

    LPH_NO_VIRTUALIZE(function()
        killnear = false
        DeSummon()
        noclip = false
        bring = false
        barragepos = false
        barragepunch = false
        vanish = false
        aura = false
        autostomp = false
        block = false
        oaura = false
        save = true
    
        if game.Players.LocalPlayer.Name == bringalt then
            while OWNER.Character.BodyEffects["K.O"].Value == true do
                Play(standsaves, true)
                while not OWNER.Character:FindFirstChild("GRABBING_CONSTRAINT") do
                    wait(.2)
                    lp.Character.HumanoidRootPart.CFrame = CFrame.new(OWNER.Character.UpperTorso.Position) * CFrame.new(0, 2.5, 0)
                    wait(.3)
                    game.ReplicatedStorage.MainEvent:FireServer("Grabbing")
                    wait(.2)
                end
                if OWNER.Character:FindFirstChild("GRABBING_CONSTRAINT") then
                    lp.Character.HumanoidRootPart.CFrame = CFrame.new(-484.67132568359, 43.499858856201, -100.71377563477)
                    wait(1.1)
                    game.ReplicatedStorage.MainEvent:FireServer("Grabbing")
                end
            end
            if OWNER.Character.BodyEffects["K.O"].Value == false then
                Summon()
                save = false
            end
    
            while OWNER.Character.BodyEffects["K.O"].Value == false and save == true and bring == true do
                Summon()
            end
        else
            Summon()
        end
    end)
    end
end)

--//------------------------------------------------------------------------------------------\\--Call all

OWNER.Chatted:Connect(function(msg)
if msg:lower() == customcallall then
    Play(standcallalls,true)
if Chatting then
game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Calling everyone!", 'All')
end
LPH_NO_VIRTUALIZE(function()
    local StarterGui = game:GetService("StarterGui")
    for i,v in pairs(game.Players:GetChildren()) do
    if v.Name ~= game.Players.LocalPlayer.Name then
    lp.Backpack["[Phone]"].Parent = lp.Character
    game.ReplicatedStorage.MainEvent:FireServer("PhoneCall", tostring(v.Name))
    task.wait()
    lp.Character["[Phone]"].Parent = lp.Backpack
    task.wait()
    local success, result = pcall(StarterGui.SetCoreGuiEnabled, StarterGui, Enum.CoreGuiType.Backpack, true)
    task.wait(.2)
    end
end
    end)()
        if Chatting then
            game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Finished calling everyone!", 'All')
        end
    end
end)

