    Spamming = true
        LPH_NO_VIRTUALIZE(function()
            spawn(function()
                while Spamming do
                    task.wait(4)
                    local message = table.concat(msg, " ", 2)
                    game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(message, "All")
                    Play(standspams,true)
                end
            end)
        end)()
            elseif msg[1]:lower() == customunspam then
        Spamming = false
    end
end)

