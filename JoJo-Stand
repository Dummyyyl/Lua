oaura           = false
    Play(standvanishs,true)
    local BP = Instance.new("BodyPosition")
    BP.MaxForce = Vector3.new(math.huge,math.huge,math.huge)
    BP.P = 2500
    BP.D = 275
    BP.Parent = lp.Character.HumanoidRootPart
    BP.Position = lp.Character.HumanoidRootPart.Position
    BP.Position = OWNER.Character.HumanoidRootPart.CFrame*CFrame.new(-4,-3.5,1.5).Position
    Look()
    wait(0.14)
    workspace.FallenPartsDestroyHeight = 0/0
    DeSummon()
    lp.Character.HumanoidRootPart.CFrame = CFrame.new(0, -999999,0)
    end
end)

