            fireclickdetector(ws_cached.Ignored.Shop["[Paintball Mask] - $62"].ClickDetector)
        end    
        local Mask = lp.Backpack:FindFirstChild("[Mask]")
        Mask.Parent = lp.Character
        Mask:Activate()
        Mask.Parent = lp.Backpack
        lp.Character:WaitForChild('In-gameMask'):FindFirstChild('Handle'):Destroy()
        lp.Character:WaitForChild('In-gameMask'):WaitForChild(""):WaitForChild("Head").CanCollide = false
        Summon()
        wait(.5)
        Summon()
        end)()
    end
end)

