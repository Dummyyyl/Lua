OWNER.Chatted:Connect(function(msg1)
    local msg = string.split(msg1," ")
    
    if msg[1]:lower() == customautokill and msg[2] then
    killnear        = false
    DeSummon()
    noclip          = false
    bring           = true
    barragepos      = false
    barragepunch    = false
    vanish          = false
    aura            = false
    autostomp       = false
    block           = false
    oaura           = false
    akill           = true
        pcall(function()
            local Target = GetPlayerByDisplayname(msg[2])
            LPH_NO_VIRTUALIZE(function()
                if Target then
                    Play(standautokills, false)
                    
                    while true do
                        local humanoidRootPart = lp.Character.HumanoidRootPart
                        local targetHrp = Target.Character.HumanoidRootPart
                        
                        if Target.Character.BodyEffects["K.O"].Value == false and not Target.Character.BodyEffects.Dead.Value == true and bring == true then
                            if lp.Backpack:FindFirstChild('Combat') then
                                lp.Backpack:FindFirstChild('Combat').Parent = lp.Character
                                lp.Character:FindFirstChild('Combat'):Activate()
                            else
                                lp.Character:FindFirstChild('Combat'):Activate()
                            end
                            for i = 1, 9 do
                                lp.Character.HumanoidRootPart.CFrame = CFrame.new(Target.Character.UpperTorso.Position) * CFrame.new(0, 5000, 50)
                                if akill == false then
                                    Summon()
                                    return
                                end
                                wait(.1)
                            end
                            wait(.01)
                            for i = 1, 30 do
                                local TargetVel = Target.Character.HumanoidRootPart.Velocity
                                local TargetPos = Target.Character.HumanoidRootPart.Position+Vector3.new(TargetVel.X*PredictionValue, 0, TargetVel.Z*PredictionValue)
                                humanoidRootPart.CFrame = CFrame.new(TargetPos)
                                if akill == false then
                                    Summon()
                                    return
                                end
                                wait(.01)
                            end
                            wait(.1)
                            for i = 1, 14 do
                                lp.Character.HumanoidRootPart.CFrame = CFrame.new(Target.Character.UpperTorso.Position) * CFrame.new(0, 5000, 0)
                                if akill == false then
                                    Summon()
                                    return
                                end
                                wait(.1)
                            end
                        end
                        
                        if bring == false then
                            return
                        end
                        
                        if task.wait(.1) and Target.Character.BodyEffects["K.O"].Value == true and bring == true and not Target.Character.BodyEffects.Dead.Value == true then
                            if game.Players.LocalPlayer.Name == bringalt then
                                lp.Character.HumanoidRootPart.CFrame = CFrame.new(Target.Character.UpperTorso.Position) * CFrame.new(0, 2.5, 0)
                                game.ReplicatedStorage.MainEvent:FireServer("Stomp")
                                if akill == false then
                                    Summon()
                                    return
                                end
                            else
                                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(0, 5000, 0)
                                if akill == false then
                                    Summon()
                                    return
                                end
                                task.wait(.1)
                            end
                        end
                        
                        if bring == true and Target.Character.BodyEffects["K.O"].Value == true and Target.Character.BodyEffects.Dead.Value == true then
                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(0, 5000, 0)
                            if akill == false then
                                Summon()
                                return
                            end
                            task.wait(1)
                        end
                        
                        if akill == false then
                            break
                        end
                    end
                    
                    task.wait(0.3)
                    lp.Character:WaitForChild("Humanoid"):ChangeState(12)
                    lp.Character.HumanoidRootPart.CFrame = OWNER.Character.HumanoidRootPart.CFrame * CFrame.new(1, 1.85, 2.5)
                    task.wait(.2)
                    lp.Character.HumanoidRootPart.CFrame = OWNER.Character.HumanoidRootPart.CFrame * CFrame.new(1, 1.85, 2.5)
                    
                    Summon()
                    block = true
                else
                    if Chatting then
                        game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Invalid Target.", 'All')
                    end
                    Summon()
                    block = true
                end
            end)()
        end)
    end
end)


OWNER.Chatted:Connect(function(msg)
    if msg:lower() == customunautokill then
       akill = false
       Play(standunautokills,true)
       Summon()
    end
end)


OWNER.Chatted:Connect(function(msg)
if msg:lower() == customautostomp then
killnear        = false
noclip          = false
bring           = false
barragepos      = false
barragepunch    = false
vanish          = false
aura            = false
autostomp       = false
block           = false
oaura           = false
Killing         = false
akill           = false
Summoned2       = true
DeSummon()
RS:BindToRenderStep("Summon", -1 , function()
    lp.Character.HumanoidRootPart.CFrame = OWNER.Character.HumanoidRootPart.CFrame*CFrame.new(2,1.85,2.5)
end)
Play(standautostomps,false)
LPH_NO_VIRTUALIZE(function()
RS:BindToRenderStep("STOMPING", 200 , function()
            pcall(function()
                    for i,v in pairs(game:GetService("Players"):GetPlayers()) do
                    if v.Character and v.Character:FindFirstChild("BodyEffects") and v.Character.BodyEffects:FindFirstChild("K.O") and v.Character.BodyEffects:FindFirstChild("Dead") and v.Character.BodyEffects["K.O"].Value == true and v.Character.BodyEffects.Dead.Value == false and v.Name ~= game.Players.LocalPlayer.Name and v.Character:FindFirstChild("UpperTorso") then
                        lp.Character.HumanoidRootPart.CFrame = CFrame.new(v.Character.UpperTorso.Position) * CFrame.new(0, 2.5, 0)
                    game:GetService('ReplicatedStorage'):FindFirstChild('MainEvent'):FireServer('Stomp')
                    else
                    end
                end
            end)
        end)
    end)()
    end
end)

