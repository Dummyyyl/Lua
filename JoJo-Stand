--//------------------------------------------------------------------------------------------\\--Anticheat Bypass

LPH_NO_VIRTUALIZE(function() 
    assert(getrawmetatable)
    gmt = getrawmetatable(game)
    setreadonly(gmt, false)
    old = gmt.__namecall
    gmt.__namecall =
    newcclosure(
    function(self, ...)
        local eventsTbl = {'DropMoney', 'TimerDecrease', 'Grabbing', 'Block', 'Stomp', 'JoinCrew', 'PhoneCall', 'Boombox', 'BoomboxStop', 'EnterPromoCode', 'PurchaseSkinCrate', 'TimerMoney', 'Reload', 'UpdateMousePos', 'FireworkText'}
        local args = {...}
            if tostring(self) == "RemoteFunction" then return end
                if tostring(self) == "MainEvent" then
                    if table.find(eventsTbl, tostring(args[1])) then
                else 
                    return
                end 
            end
        return old(self, ...)
    end)
end)()

--//------------------------------------------------------------------------------------------\\--Anti Crash

LPH_NO_VIRTUALIZE(function() 
local g
g = hookmetamethod(game, "__namecall", function(...)
local f = {
    ...
}
local c = f[1]
local a = getnamecallmethod()
local _ = getcallingscript()
if (a == "FireServer" and c == d and e(b, f[2])) then
    return
end
    if (not checkcaller() and getfenv(2).crash) then
        hookfunction(getfenv(2).crash, function()
            end)
        end
    return g(...)
end)
end)()

--//------------------------------------------------------------------------------------------\\--Anti Adminban

game:GetService("Players").PlayerAdded:Connect(function(Player)
    if Player:WaitForChild("Name") == PRIVATESTAND['MAIN']['OWNER'] then
    OWNER = Player
end
    if Player:WaitForChild("Backpack"):FindFirstChild("AdminBan") then
        lp:Kick("Admin has joined the server.")
    end
end)

--//------------------------------------------------------------------------------------------\\--Anti AdminBan

for i,v in pairs(game:GetService("Players"):GetPlayers()) do
    if v and v:WaitForChild("Backpack"):FindFirstChild("AdminBan") then
        lp:Kick("Admin was in the server.")
    end
end

--//------------------------------------------------------------------------------------------\\--Hide Screen

LPH_NO_VIRTUALIZE(function()
coroutine.wrap(function()
    local g = game
    local w = g.Workspace
    local l = g:GetService"Lighting"
    local t = w:WaitForChild"Terrain"
    t.WaterWaveSize = 0
    t.WaterWaveSpeed = 0
    t.WaterReflectance = 0
    t.WaterTransparency = 1
    l.GlobalShadows = false
    settings().Physics.PhysicsEnvironmentalThrottle = 1
    settings().Rendering.QualityLevel = 'Level01'
    for i,v in pairs(game:GetDescendants()) do
        if v:IsA("Part") then
            v.Material = Enum.Material.Pavement
            v.Transparency = 1
        elseif v:IsA("Decal") then
            v:Destroy()
        elseif v:IsA("Texture") then
            v:Destroy()
        elseif v:IsA("MeshPart") then
            v.TextureID = 0
            v.Transparency = 1
        elseif v.Name == "Terrian" then
            v.WaterReflectace = 1
            v.WaterTransparency = 1
        elseif v:IsA("SpotLight") then
            v.Range = 0
            v.Enabled = false
        elseif v:IsA("WedgePart") then
            v.Transparency = 1
        elseif v:IsA("UnionOperation") then
            v.Transparency = 1
            LOADED = true
        end
    end
end)()
end)()
--//------------------------------------------------------------------------------------------\\--Respawn Function

local function Respawn()
lp.Character:WaitForChild("ForceField")
lp.Character:WaitForChild("Humanoid")
lp.Character:WaitForChild("Head")
lp.Character:WaitForChild("BodyEffects")

--//------------------------------------------------------------------------------------------\\--Remove Animation

lp.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Climbing, false)
lp.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Jumping, false)
lp.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Landed, false)
lp.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Freefall, false)
lp.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Seated, false)
lp.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.PlatformStanding, false)
lp.Character:FindFirstChild("Animate").Disabled = false
lp.Character.Humanoid.AutoRotate = false

--//------------------------------------------------------------------------------------------\\--Auto Mask
if automask then
    task.spawn(function() 
        if Paintballmask then 
            repeat task.wait() lp.Character.HumanoidRootPart.CFrame = CFrame.new(game:GetService("Workspace").Ignored.Shop["[Paintball Mask] - $62"].Head.Position) fireclickdetector(game:GetService("Workspace").Ignored.Shop["[Paintball Mask] - $62"].ClickDetector) until lp.Backpack:FindFirstChild("[Mask]")
        elseif SurgeonMask then 
            repeat task.wait() lp.Character.HumanoidRootPart.CFrame = CFrame.new(game:GetService("Workspace").Ignored.Shop["[Surgeon Mask] - $26"].Head.Position) fireclickdetector(game:GetService("Workspace").Ignored.Shop["[Surgeon Mask] - $26"].ClickDetector) until lp.Backpack:FindFirstChild("[Mask]")
        end
        Mask = lp.Backpack:FindFirstChild("[Mask]")
        Combat = lp.Backpack:WaitForChild("Combat")
        Mask.Parent = lp.Character
        Mask:Activate()
        Mask.Parent = lp.Backpack 
        Combat.Parent = lp.Character 
        lp.Character:WaitForChild('In-gameMask'):FindFirstChildWhichIsA("Model"):Destroy()
        lp.Character:WaitForChild('In-gameMask'):FindFirstChild('Handle'):Destroy()
    end)
end

--//------------------------------------------------------------------------------------------\\--
function AnimPlay(ID, SPEED)
local animation = Instance.new('Animation', ws_cached)
animation.AnimationId = 'rbxassetid://'..ID
playing = lp.Character:WaitForChild("Humanoid"):LoadAnimation(animation)
playing:Play() 
    if tonumber(SPEED) then
        playing:AdjustSpeed(SPEED)
    else
        playing:AdjustSpeed(1)
    end
animation:Destroy()
end
AnimPlay(3541114300,0.8)
--//------------------------------------------------------------------------------------------\\--
function AnimStop(ID, SPEED)
    for i,v in pairs(lp.Character:WaitForChild("Humanoid"):GetPlayingAnimationTracks()) do
        if v.Animation.AnimationId == 'rbxassetid://'..ID then
        if tonumber(SPEED) then
                v:Stop(SPEED)
            else
                v:Stop()
            end
        end
    end
end
--//------------------------------------------------------------------------------------------\\-- Animations :

if not game:GetService("Workspace"):FindFirstChild("Gliding") then
    Glide = Instance.new('Animation', game:GetService("Workspace"))
if MoveGlide then
    Glide.AnimationId = 'rbxassetid://3084858603'
elseif MoveFly then
    Glide.AnimationId = 'rbxassetid://3541044388'
else
    Glide.AnimationId = 'rbxassetid://3084858603'
end
    Glide.Name = 'Gliding'
    Glide2 = lp.Character:FindFirstChildWhichIsA('Humanoid'):LoadAnimation(Glide)
    else
    Glide = game:GetService("Workspace").Gliding
    Glide2 = lp.Character:FindFirstChildWhichIsA('Humanoid'):LoadAnimation(Glide)
end 

--//------------------------------------------------------------------------------------------\\--
if not ws_cached:FindFirstChild("barrage1") and not ws_cached:FindFirstChild("barrage2") and not ws_cached:FindFirstChild("barrage3") and not ws_cached:FindFirstChild("barrage4") then
local barrage1 = Instance.new('Animation', ws_cached)
barrage1.AnimationId = 'rbxassetid://8254787838'
barrage1.Name = "barrage1"
local barrage2 = Instance.new('Animation', ws_cached)
barrage2.AnimationId = 'rbxassetid://8254789608'
barrage2.Name = "barrage2"
local barrage3 = Instance.new('Animation', ws_cached)
barrage3.AnimationId = 'rbxassetid://8254792138'
barrage3.Name = "barrage3"
local barrage4 = Instance.new('Animation', ws_cached)
barrage4.AnimationId = 'rbxassetid://8254794168'
barrage4.Name = "barrage4"
Barraging1 = lp.Character:FindFirstChildWhichIsA('Humanoid'):LoadAnimation(barrage1)
Barraging2 = lp.Character:FindFirstChildWhichIsA('Humanoid'):LoadAnimation(barrage2)
Barraging3 = lp.Character:FindFirstChildWhichIsA('Humanoid'):LoadAnimation(barrage3)
Barraging4 = lp.Character:FindFirstChildWhichIsA('Humanoid'):LoadAnimation(barrage4)
else
local barrage1 = ws_cached.barrage1
local barrage2 = ws_cached.barrage2
local barrage3 = ws_cached.barrage3
local barrage4 = ws_cached.barrage4
Barraging1 = lp.Character:FindFirstChildWhichIsA('Humanoid'):LoadAnimation(barrage1)
Barraging2 = lp.Character:FindFirstChildWhichIsA('Humanoid'):LoadAnimation(barrage2)
Barraging3 = lp.Character:FindFirstChildWhichIsA('Humanoid'):LoadAnimation(barrage3)
Barraging4 = lp.Character:FindFirstChildWhichIsA('Humanoid'):LoadAnimation(barrage4)
end

