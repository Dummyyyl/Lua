--//-----------------------------------------------------------------------------------------\\--Get Player

local function GetPlayerByDisplayname(String)
local plr
local strl = String:lower()
    for i, v in pairs(game:GetService("Players"):GetPlayers()) do
        if v.Name:lower():sub(1, #String) == String:lower() or v.DisplayName:lower():sub(1, #String) == String:lower() then
            plr = v
        end 
    end
if String == "" or String == " " then
   plr = nil
end
    return plr
end

--//-----------------------------------------------------------------------------------------\\--PREDICTION

local pingvalue = nil;
local split = nil;
local ping = nil;

local PredictionValue = 0.31

local AutoPrediction = true

game:GetService("RunService").Stepped:connect(function()
    if AutoPrediction == true then
        pingvalue = game:GetService("Stats").Network.ServerStatsItem["Data Ping"]:GetValueString()
        split = string.split(pingvalue,'(')
        ping = tonumber(split[1])
        if ping < 300 then
            PredictionValue = 0.445
        elseif ping < 280 then
            PredictionValue = 0.445
        elseif ping < 260 then
            PredictionValue = 0.445
        elseif ping < 240 then
            PredictionValue = 0.445
        elseif ping < 220 then
            PredictionValue = 0.445
        elseif ping < 200 then
            PredictionValue = 0.435
        elseif ping < 180 then
            PredictionValue = 0.42
        elseif ping < 160 then
            PredictionValue = 0.42
        elseif ping < 140 then
            PredictionValue = 0.35
        elseif ping < 120 then
            PredictionValue = 0.3
        elseif ping < 100 then
            PredictionValue = 0.28
        elseif ping < 80 then
            PredictionValue = 0.29
        elseif ping < 60 then
            PredictionValue = 0.29
        elseif ping < 40 then
            PredictionValue = 0.29
        end
    end
end)

--//-----------------------------------------------------------------------------------------\\--
LPH_NO_VIRTUALIZE(function()
    game:GetService('RunService').Stepped:connect(function()
        if killnear == true then
            local Target = GetNearest()
            local humanoidRootPart = lp.Character.HumanoidRootPart
            local targetHrp = Target.Character.HumanoidRootPart
            local targetchar = Target.Character
            if Target then
                if targetchar.BodyEffects['K.O'].Value == false then
                    local TargetVel = Target.Character.HumanoidRootPart.Velocity
                    local TargetPos = Target.Character.HumanoidRootPart.Position+Vector3.new(TargetVel.X*PredictionValue, 0, TargetVel.Z*PredictionValue)
                    humanoidRootPart.CFrame = CFrame.new(TargetPos)
                Hit()
            end
        end
    end
end)
end)()

--//------------------------------------------------------------------------------------------\\-- Ability Functions :

local function Look()
    local direction = OWNER.Character.HumanoidRootPart.Position + OWNER.Character.HumanoidRootPart.CFrame.LookVector * OWNER.Character.HumanoidRootPart.Position.Magnitude
    lp.Character.HumanoidRootPart.CFrame = CFrame.lookAt(lp.Character.HumanoidRootPart.Position, direction)
end

local function Summon()
    AnimPlay(3541114300,0.8)
        block = true
            LPH_NO_VIRTUALIZE(function()
                    if lp.Character.HumanoidRootPart:FindFirstChildWhichIsA("BodyPosition") then
                        lp.Character.HumanoidRootPart:FindFirstChildWhichIsA("BodyPosition"):Destroy()
                    end
                Summoned = true
            local BP = Instance.new("BodyPosition")
            BP.MaxForce = Vector3.new(math.huge,math.huge,math.huge)
            BP.P = 2500
            BP.D = 275
            BP.Parent = lp.Character.HumanoidRootPart
            BP.Position = lp.Character.HumanoidRootPart.Position
            game:GetService('RunService').Stepped:connect(function()
                if Summoned == true and Pos == "Right" then
                    BP.Position = OWNER.Character.HumanoidRootPart.CFrame*CFrame.new(2,1.85,2.5).Position
                    Moved()
                    Look()
                elseif Summoned == true and Pos == "Middle" then
                    BP.Position = OWNER.Character.HumanoidRootPart.CFrame*CFrame.new(0,1.85,2.5).Position
                    Moved()
                    Look()
                elseif Summoned == true and Pos == "Left" then
                    BP.Position = OWNER.Character.HumanoidRootPart.CFrame*CFrame.new(-2,1.85,2.5).Position
                    Moved()
                    Look()
                end
            end)
    end)()
end

local function DeSummon()
    if lp.Character.HumanoidRootPart:FindFirstChildWhichIsA("BodyPosition") then
    lp.Character.HumanoidRootPart:FindFirstChildWhichIsA("BodyPosition"):Destroy()
    end
    RS:UnbindFromRenderStep("Summon")
    RS:UnbindFromRenderStep("STOMPING")
    Summoned2 = false
    Summoned = false
end

--//------------------------------------------------------------------------------------------\\--Stand Summon2

OWNER.Chatted:Connect(function(msg)
    if msg:lower() == customsummon then
        DeSummon()
        killnear        = false
        noclip          = false
        bring           = false
        barragepos      = false
        barragepunch    = false
        vanish          = false
        aura            = false
        autostomp       = false
        block           = true
        oaura           = false
    LPH_NO_VIRTUALIZE(function()
        lp.Character.HumanoidRootPart.CFrame = OWNER.Character.HumanoidRootPart.CFrame*CFrame.new(-8,-8,1.5)
        wait(0.1)
        lp.Character.HumanoidRootPart.CFrame = OWNER.Character.HumanoidRootPart.CFrame*CFrame.new(-8,-8,1.5)
        Summon()
        Play(standsummons,true)
        end)()
    end
end)

