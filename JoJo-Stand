            while not game.Players.LocalPlayer.Backpack:FindFirstChild("[Money Gun]") do
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Ignored.Shop["[Money Gun] - $800"].Head.CFrame
                fireclickdetector(game:GetService("Workspace").Ignored.Shop["[Money Gun] - $800"].ClickDetector)
                task.wait(0.1)
            end
            wait()
            Summon()
        end
    end
end)

