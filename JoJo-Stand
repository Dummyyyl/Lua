    end
end)

--//-------------------------------------------------------------------------------------------------------------\\--Save

local function Save()
LPH_NO_VIRTUALIZE(function()
        killnear        = false
        DeSummon()
        noclip          = false
        bring           = false
        barragepos      = false
        barragepunch    = false
        vanish          = false
        aura            = false
        autostomp       = false
        block           = false
        oaura           = false
        save            = true
        if game.Players.LocalPlayer.Name == bringalt then
            while true do
                wait()
                while OWNER.Character.BodyEffects["K.O"].Value == true do
                    wait(.3)
                    lp.Character.HumanoidRootPart.CFrame = CFrame.new(OWNER.Character.UpperTorso.Position) * CFrame.new(0,2.5,0)
                    wait(.1)
                    lp.Character.HumanoidRootPart.CFrame = CFrame.new(OWNER.Character.UpperTorso.Position) * CFrame.new(0,2.5,0)
                    wait(.1)
                    lp.Character.HumanoidRootPart.CFrame = CFrame.new(OWNER.Character.UpperTorso.Position) * CFrame.new(0,2.5,0)
                    game.ReplicatedStorage.MainEvent:FireServer("Grabbing")
                    wait(.2)
                    
                    while not OWNER.Character:FindFirstChild("GRABBING_CONSTRAINT") and bring == true and save == true do
                        wait()
                    end
                    
                    if OWNER.Character:FindFirstChild("GRABBING_CONSTRAINT") then
                        lp.Character.HumanoidRootPart.CFrame = CFrame.new(-484.67132568359, 43.499858856201, -100.71377563477)
                        wait(1.1)
                        game.ReplicatedStorage.MainEvent:FireServer("Grabbing")
                    end
                end
                if OWNER.Character.BodyEffects["K.O"].Value == false then
                    Summon()
                    asave = false
                end
                if OWNER.Character.BodyEffects["K.O"].Value == false or save == false or bring == false or asave == false then
                    break
                end
            end   
        else
        Summon()
    end
    end)()
end

--//-----------------------------------------------------------------------------------------\\--

local debounce = false

game:GetService('RunService').Heartbeat:Connect(function()
    LPH_NO_VIRTUALIZE(function()
        if asave and not debounce then
            while OWNER.Character.BodyEffects["K.O"].Value == true do
                debounce = true
                Save()
                Play(standautosaves, true)
                print("true")
                wait(.4)
                debounce = false
                save = false
                while OWNER.Character.BodyEffects["K.O"].Value == true and not OWNER.Character:FindFirstChild("GRABBING_CONSTRAINT") do
                    wait()
                end
            end
            asave = true
        end
    end)()
end)

