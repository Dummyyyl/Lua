--//-------------------------------------------------------------------------------------------------------------\\--Bring to owner

OWNER.Chatted:Connect(function(msg1)
local msg = string.split(msg1," ")

if msg[1]:lower() == custombring and msg[2] then
killnear        = false
DeSummon()
noclip          = false
bring           = true
barragepos      = false
barragepunch    = false
vanish          = false
aura            = false
autostomp       = false
block           = false
oaura           = false

local function grabTarget(target)
lp.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Seated)
lp.Character.HumanoidRootPart.CFrame = CFrame.new(target.Character.UpperTorso.Position) * CFrame.new(0,2.5,0)
game.ReplicatedStorage.MainEvent:FireServer("Grabbing")
end

pcall(function()
    local Target = GetPlayerByDisplayname(msg[2])
        LPH_NO_VIRTUALIZE(function()
            if Target then
                local humanoidRootPart = lp.Character.HumanoidRootPart
                local targetHrp = Target.Character.HumanoidRootPart
                Play(standbrings,true)
            while Target.Character.BodyEffects["K.O"].Value == false and bring == true do
                if lp.Backpack:FindFirstChild('Combat') then
                    lp.Backpack:FindFirstChild('Combat').Parent = lp.Character
                    lp.Character:FindFirstChild('Combat'):Activate()
                else
                    lp.Character:FindFirstChild('Combat'):Activate()
                end
                for i = 1,9 do
                    lp.Character.HumanoidRootPart.CFrame = CFrame.new(Target.Character.UpperTorso.Position) * CFrame.new(0,5000,0)
                    wait(.1)
                end
                wait(.01)
                for i = 1,30 do
                    local TargetVel = Target.Character.HumanoidRootPart.Velocity
                    local TargetPos = Target.Character.HumanoidRootPart.Position+Vector3.new(TargetVel.X*PredictionValue, 0, TargetVel.Z*PredictionValue)
                    humanoidRootPart.CFrame = CFrame.new(TargetPos)
                    wait(.01)
                end
                wait(.1)
                for i = 1,14 do
                    lp.Character.HumanoidRootPart.CFrame = CFrame.new(Target.Character.UpperTorso.Position) * CFrame.new(0,5000,0)
                    wait(.1)
                end
            end
            if bring == false then return end
            if game.Players.LocalPlayer.Name == bringalt then
                while Target.Character.BodyEffects["K.O"].Value == true and bring == true and Target.Character.BodyEffects.Dead.Value == false do
                    for i = 1,5 do
                        lp.Character.HumanoidRootPart.CFrame = CFrame.new(Target.Character.UpperTorso.Position) * CFrame.new(0,2.5,0)
                        task.wait(.1)
                    end
                grabTarget(Target)
                task.wait(0.5)
            end
            else
            Summon()
            block = true
            end
            task.wait(0.3)
            lp.Character:WaitForChild("Humanoid"):ChangeState(12)
            lp.Character.HumanoidRootPart.CFrame = OWNER.Character.HumanoidRootPart.CFrame*CFrame.new(2,1.85,2.5)
            task.wait(.2)
            lp.Character.HumanoidRootPart.CFrame = OWNER.Character.HumanoidRootPart.CFrame*CFrame.new(2,1.85,2.5)
            Summon()

            block = true
            else
                if Chatting then
                    game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Invalid Target.", 'All')
                end
                Summon()
                block = true
            end
        end)()
    end)    
end
end)

--//-------------------------------------------------------------------------------------------------------------\\--Stomp Cmd 

OWNER.Chatted:Connect(function(msg1)
    local msg = string.split(msg1," ")
    if msg[1]:lower() == customstomp and msg[2] then
        killnear        = false
        DeSummon()
        noclip          = false
        bring           = true
        barragepos      = false
        barragepunch    = false
        vanish          = false
        aura            = false
        autostomp       = true
        block           = false
        oaura           = false
        Killing = true
        pcall(function()
            local Target = GetPlayerByDisplayname(msg[2])
            LPH_NO_VIRTUALIZE(function()
                if Target then
                    Play(standstomps,true)
                local humanoidRootPart = lp.Character.HumanoidRootPart
                local targetHrp = Target.Character.HumanoidRootPart
                while Target.Character.BodyEffects["K.O"].Value == false and bring == true do
                    DeSummon()
                
                    if lp.Backpack:FindFirstChild('Combat') then
                        lp.Backpack:FindFirstChild('Combat').Parent = lp.Character
                        lp.Character:FindFirstChild('Combat'):Activate()
                    else
                        lp.Character:FindFirstChild('Combat'):Activate()
                    end
                    for i = 1,9 do
                        lp.Character.HumanoidRootPart.CFrame = CFrame.new(Target.Character.UpperTorso.Position) * CFrame.new(0,5000,0)
                        wait(.1)
                    end
                    wait(.01)
                    for i = 1,30 do
                        local TargetVel = Target.Character.HumanoidRootPart.Velocity
                        local TargetPos = Target.Character.HumanoidRootPart.Position+Vector3.new(TargetVel.X*PredictionValue, 0, TargetVel.Z*PredictionValue)
                        humanoidRootPart.CFrame = CFrame.new(TargetPos)
                        wait(.01)
                    end
                    wait(.1)
                    for i = 1,14 do
                        lp.Character.HumanoidRootPart.CFrame = CFrame.new(Target.Character.UpperTorso.Position) * CFrame.new(0,5000,0)
                        wait(.1)
                    end
                end
                if bring == false then return end
                lp.Character:WaitForChild("Humanoid"):ChangeState(11)
                if game.Players.LocalPlayer.Name == bringalt then
                    while not Target.Character.BodyEffects.Dead.Value and bring == true and not Target.Character:FindFirstChild("GRABBING_CONSTRAINT") do
                        lp.Character.HumanoidRootPart.CFrame = CFrame.new(Target.Character.UpperTorso.Position) * CFrame.new(0, 2.5, 0)
                        wait(.1)
                    end                    
            else
                
                Summon()
                autostomp = false
            end
                autostomp = false
                Killing = false
                lp.Character:WaitForChild("Humanoid"):ChangeState(12)
                humanoidRootPart.CFrame = OWNER.Character.HumanoidRootPart.CFrame*CFrame.new(2,1.85,2.5)
                task.wait(.2)
                humanoidRootPart.CFrame = OWNER.Character.HumanoidRootPart.CFrame*CFrame.new(2,1.85,2.5)
                Summon()
                
                block = true
        else
            if Chatting then
                game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Invalid Target.", 'All')
            end
            Summon()
            
            block = true
            autostomp = false
            Killing = false
        end
        end)()
    end)
    end
end)

--//-------------------------------------------------------------------------------------------------------------\\--Goto

OWNER.Chatted:Connect(function(msg1)
local msg = string.split(msg1," ")
if msg[1]:lower() == customgoto and msg[2] then
killnear        = false
DeSummon()
noclip          = false
bring           = true
barragepos      = false
barragepunch    = false
vanish          = false
aura            = false
autostomp       = false
block           = false
oaura           = false
pcall(function()
    local Target = GetPlayerByDisplayname(msg[2])
    LPH_NO_VIRTUALIZE(function()
        Play(standgotos,true)
        
        while not (OWNER.Character.BodyEffects["K.O"].Value == true or bring == false) do
            task.wait()
            game.ReplicatedStorage.MainEvent:FireServer("LeaveCrew")
            if lp.Backpack:FindFirstChild('Combat') then
                lp.Backpack:FindFirstChild('Combat').Parent = lp.Character
                lp.Character:FindFirstChild('Combat'):Activate()
            else
                lp.Character:FindFirstChild('Combat'):Activate()
            end
            wait(.001)
        
            DeSummon()
            lp.Character.HumanoidRootPart.Position = OWNER.Character.HumanoidRootPart.Position + Vector3.new(OWNER.Character.HumanoidRootPart.Velocity.X/2.75, 0, OWNER.Character.HumanoidRootPart.Velocity.Z/2.75)
        end
        
        if game.Players.LocalPlayer.Name == bringalt then
            while not (OWNER.Character:FindFirstChild("GRABBING_CONSTRAINT") or bring == false) do
                task.wait(.5)
                lp.Character:WaitForChild("Humanoid"):ChangeState(11)
                lp.Character.HumanoidRootPart.CFrame = CFrame.new(OWNER.Character.UpperTorso.Position) * CFrame.new(0,2.5,0)
                task.wait(0.3)
                game.ReplicatedStorage.MainEvent:FireServer("Grabbing")
                task.wait(0.5)
            end
        end
        
        local Target = GetPlayerByDisplayname(msg[2])
        
        if Target then
            lp.Character.HumanoidRootPart.CFrame = Target.Character.HumanoidRootPart.CFrame * CFrame.new(5, 0, 0)
        elseif msg[2] == "bank" then
            lp.Character.HumanoidRootPart.CFrame = CFrame.new(-438.689, 39.0127, -284.731)
        elseif msg[2] == "club" then
            lp.Character.HumanoidRootPart.CFrame = CFrame.new(-265.43808, 0.0313372575, -335.743134)
        elseif msg[2] == "train" then
            lp.Character.HumanoidRootPart.CFrame = CFrame.new(602.676086, 47.9999886, -109.291306)
        elseif msg[2] == "admin" then
            lp.Character.HumanoidRootPart.CFrame = CFrame.new(-872.135864, -32.6492119, -532.292603)
        elseif msg[2] == "school" then
            lp.Character.HumanoidRootPart.CFrame = CFrame.new(-518.986145, 68.6249771, 272.222656)
        elseif msg[2] == "casino" then
            lp.Character.HumanoidRootPart.CFrame = CFrame.new(-1005.84552, 49.5999413, -140.588593)
        elseif msg[2] == "ufo" then
            lp.Character.HumanoidRootPart.CFrame = CFrame.new(69.6755676, 138.999939, -639.045044)
        else
            if Chatting then
                game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Invalid Target or Place going: Bank", 'All')
            end
            lp.Character.HumanoidRootPart.CFrame = CFrame.new(-438.689, 39.0127, -284.731)
        end
        
        lp.Character:WaitForChild("Humanoid"):ChangeState(12)
        wait(.4)
        game.ReplicatedStorage.MainEvent:FireServer("Grabbing")
        wait(.3)
        
        Summon()
        block = true
        print("true")
            end)()
        end)
    end
end)

